/* miniprogram/pages/transaction/transaction.wxss */
.select_tip{
  display: flex;
  border: 5rpx solid white;
  border-radius: 20rpx;
  background-color: #FADA61;
  justify-content: center;
  align-items: center;
  /* width: 70%; */
  height: 100rpx;
}
.account_items{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border: 5rpx solid black;
  border-radius: 25rpx;
  margin: 10rpx;
  background-color: #FADA61;
  height: 100rpx;
}
.texts{
  margin-left: 7rpx;
  /* margin-right: 500rpx; */
}
/* miniprogram/pages/income/income.wxss */
.item_bars{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 20rpx;
  border: 5rpx solid black;
  border-radius: 25rpx;
  background-color: #FADA61;
  height: 70rpx;
  flex-flow:row;
}
.item_bars view{
  width:50%;
  display:inline-block;
  }
.right{
  text-align:right;
  padding-right:30rpx;
  }
.remain{
  border: 5rpx solid black;
  margin: 20rpx 20rpx 20rpx 20rpx;
  border-radius: 25rpx;
  background-color: #FADA61;
}
.remain_item{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 15rpx;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
  flex-flow:row;
}
.remain_item view{
  width:50%;
  display:inline-block;
  }
.texts{
  margin-left: 10rpx;
  /* border-bottom: 3rpx solid #FADA61; */
}
.submit_btn{
  width: 80%;
  margin-top: 50rpx;
  border-radius: 60rpx;
  background-color: #FADA61;
}
.submit_btn::after{
  border: 0;
}
.divline{
  background: #FADA61;
  width: 100%;
  height: 5rpx;
  margin-top: 10rpx;
}
