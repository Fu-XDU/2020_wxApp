/* 输入框布局*/
.container1{
  width: 600rpx;
  height: 500rpx;
  /* background-color: red; */
  margin: 0 auto;
  display: flex;
  flex-direction: column;
}
.container1 .form_item{
  flex: 1;
  display: flex;
  /* background-color: pink; */
  /* border-bottom: 1px solid purple; */
  margin: 20rpx 0;
}
.name,.zhuanyeyukecheng,.question-feedba,
.account{
  border-bottom: 1px solid black;
  flex: 1;
  margin-left: 22rpx;
}

.reserve_button{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
}

