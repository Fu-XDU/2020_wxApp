/* miniprogram/pages/expenditure/expenditure.wxss */
.bold{
  font-weight: bold;
}
.texts{
  margin-left: 12rpx;
  color: #fff;
}
.icons{
  margin-left: 10rpx;
  margin-right: 20rpx;
}
.each_item{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
}
.item_bars_1{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 20rpx;
  border: 5rpx solid white;
  border-radius: 25rpx;
  background-color: #288af5;
  height: 100rpx;
}
.item_bars view{
  width:50%;
  display:inline-block;
}
.right{
  text-align:right;
  /* padding-right:30rpx; */
  color: #fff;
}
.item_bars_2{
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin: 20rpx;
    border: 5rpx solid white;
    border-radius: 25rpx;
    background-color: #288af5;
    height: 200rpx;
  }
.texts{
  margin-left: 10rpx;
  /* border-bottom: 3rpx solid black; */
  color: #fff;
}
.submit_btn{
  width: 80%;
  margin-top: 50rpx;
  border-radius: 60rpx;
  /* background-color: #FADA61; */
  background-color: #288af5;
  color: #fff;
}
.submit_btn::after{
  border: 0;
}
