/* miniprogram/pages/income/income.wxss */
.item_bars{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 20rpx;
  border: 5rpx solid black;
  border-radius: 25rpx;
  background-color: #FADA61;
  height: 70rpx;
}
.remain{
  border: 5rpx solid black;
  margin: 20rpx 10rpx 20rpx 10rpx;
  border-radius: 45rpx;
  background-color: #FADA61;
}
.remain_item{
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
}
.texts{
  margin-left: 10rpx;
  /* border-bottom: 3rpx solid #FADA61; */
}
.submit_btn{
  width: 80%;
  margin-top: 50rpx;
  border-radius: 60rpx;
  background-color: #FADA61;
}
.submit_btn::after{
  border: 0;
}
